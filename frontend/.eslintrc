{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "implicit-arrow-linebreak": "off",
    "react/forbid-prop-types": "off",
    "react/jsx-one-expression-per-line":  "off",
    "react/prop-types":  "off",
    "comma-dangle": "off",
    "indent": "warn",
    "no-trailing-spaces": "off"
  }
}